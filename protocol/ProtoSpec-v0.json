{
    "MajorVersion": "0",
    "MinorVersion": [
        {
            "Extension": {
                "Load": {
                    "Command": "PROTO-0-C-EXT-LOAD",
                    "Argument": [ "Name", "MajorVersion" ],
                    "Return": {
                        "Loaded": "PROTO-0-R-EXT-LOADED"
                    },
                    "Error": "PROTO-0-E-EXT-LOAD"
                },
                "Unload": {
                    "Command": "PROTO-0-C-EXT-UNLOAD",
                    "Argument": [ "Name", "MajorVersion" ],
                    "Return": {
                        "Unloaded": "PROTO-0-R-EXT-UNLOADED"
                    },
                    "Error": "PROTO-0-E-EXT-UNLOAD"
                }
            },
            "APP": {
                "List": {
                    "Command": "PROTO-0-C-APP-LIST",
                    "Argument": [],
                    "Return": {
                        "List": "PROTO-0-R-APP-LIST"
                    },
                    "Error": "PROTO-0-E-APP-LIST"
                },
                "Install": {
                    "Command": "PROTO-0-C-APP-INSTALL",
                    "Argument": [ "AppBundleDataStream" ],
                    "Return": {
                        "Uploading": "PROTO-0-R-APP-UPLOADING",
                        "Installing": "PROTO-0-R-APP-INSTALLING",
                        "Installed": "PROTO-0-R-APP-INSTALLED"
                    },
                    "Error": "PROTO-0-E-APP-INSTALL"
                },
                "CancelInstall": {
                    "Command": "PROTO-0-C-APP-CANCEL",
                    "Argument": [ "InstallationCode" ],
                    "Return": {
                        "Cancelled": "PROTO-0-R-APP-CANCELLED"
                    },
                    "Error": "PROTO-0-E-APP-CANCEL"
                },
                "Uninstall": {
                    "Command": "PROTO-0-C-APP-UNINSTALL",
                    "Argument": [ "AppInfo" ],
                    "Return": {
                        "Uninstalled": "PROTO-0-R-APP-UNINSTALLED"
                    },
                    "Error": "PROTO-0-E-APP-UNINSTALL"
                }
            },
            "Storage": {
                "GetLocalDisks": {
                    "Command": "PROTO-0-C-STOR-GET-LOCAL-DISKS",
                    "Argument": [],
                    "Return": {
                        "Disks": "PROTO-0-R-STOR-LOCAL-DISKS"
                    },
                    "Error": "PROTO-0-E-STOR-GET-LOCAL-DISKS"
                }
            },
            "FileSystem": {
                "List": {
                    "Command": "PROTO-0-C-FS-LIST",
                    "Argument": [ "Path" ],
                    "Return": {
                        "List": "PROTO-0-R-FS-LIST"
                    },
                    "Error": "PROTO-0-E-FS-LIST"
                },
                "CreateFile": {
                    "Command": "PROTO-0-C-FS-CREATE-FILE",
                    "Argument": [ "Path" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-CREATE-FILE"
                },
                "CreateDirectory": {
                    "Command": "PROTO-0-C-FS-CREATE-DIR",
                    "Argument": [ "Path" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-CREATE-DIR"
                },
                "CreateHardLink": {
                    "Command": "PROTO-0-C-FS-CREATE-HARDLINK",
                    "Argument": [ "SrcPath", "DstPath" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-CREATE-HARDLINK"
                },
                "CreateSymbolicLink": {
                    "Command": "PROTO-0-C-FS-CREATE-SYMLINK",
                    "Argument": [ "SrcPath", "DstPath" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-CREATE-SYMLINK"
                },
                "Remove": {
                    "Command": "PROTO-0-C-FS-REMOVE",
                    "Argument": [ "Path" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-REMOVE"
                },
                "Move": {
                    "Command": "PROTO-0-C-FS-MOVE",
                    "Argument": [ "SrcPath", "DstPath" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-MOVE"
                },
                "Copy": {
                    "Command": "PROTO-0-C-FS-COPY",
                    "Argument": [ "SrcPath", "DstPath" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-COPY"
                },
                "Exists": {
                    "Command": "PROTO-0-C-FS-EXISTS",
                    "Argument": [ "Path" ],
                    "Return": {
                        "True": true,
                        "False": false
                    },
                    "Error": "PROTO-0-E-FS-EXISTS"
                },
                "ReadFile": {
                    "Command": "PROTO-0-C-FS-READ-FILE",
                    "Argument": [ "Path" ],
                    "Return": {
                        "Data": "PROTO-0-R-FS-READ-DATA"
                    },
                    "Error": "PROTO-0-E-FS-READ-FILE"
                },
                "WriteFile": {
                    "Command": "PROTO-0-C-FS-WRITE-FILE",
                    "Argument": [ "Path", "Data" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-WRITE-FILE"
                },
                "AppendFile": {
                    "Command": "PROTO-0-C-FS-APPEND-FILE",
                    "Argument": [ "Path", "Data" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FS-APPEND-FILE"
                }
            },
            "FileHandle": {
                "Open": {
                    "Command": "PROTO-0-C-FILE-OPEN",
                    "Argument": [ "Path" ],
                    "Return": {
                        "FileHandle": "PROTO-0-R-FILE-OPEN"
                    },
                    "Error": "PROTO-0-E-FILE-OPEN"
                },
                "Close": {
                    "Command": "PROTO-0-C-FILE-CLOSE",
                    "Argument": [ "FileHandle" ],
                    "Return": {},
                    "Error": "PROTO-0-E-FILE-CLOSE"
                },
                "ReadData": {
                    "Command": "PROTO-0-C-FILE-READ-DATA",
                    "Argument": [ "FileHandle", "Offset", "Size" ],
                    "Return": {
                        "Data": "PROTO-0-R-FILE-READ-DATA"
                    },
                    "Error": "PROTO-0-E-FILE-READ-DATA"
                },
                "WriteData": {
                    "Command": "PROTO-0-C-FILE-WRITE-DATA",
                    "Argument": [ "FileHandle", "Offset", "Size", "Data" ],
                    "Return": {
                        "Size": "PROTO-0-R-FILE-WRITE-DATA-SIZE"
                    },
                    "Error": "PROTO-0-E-FILE-WRITE-DATA"
                },
                "AppendData": {
                    "Command": "PROTO-0-C-FILE-APPEND-DATA",
                    "Argument": [ "FileHandle", "Data" ],
                    "Return": {
                        "Size": "PROTO-0-R-FILE-APPEND-DATA-SIZE"
                    },
                    "Error": "PROTO-0-E-FILE-APPEND-DATA"
                }
            }
        }
    ]
}
